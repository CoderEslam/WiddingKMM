<resources>

    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/kashmirdark</item>
        <item name="colorPrimaryDark">@color/foshialight</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="styleCard">
        <item name="cardBackgroundColor">@color/babykshmir</item>
        <item name="background">@color/babykshmir</item>
    </style>

    <style name="textStyle">
        <item name="android:textColor">@color/kashmirdark</item>
    </style>

    <style name="buttonStyle">
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="TextAttachmentOptions">
        <item name="android:fontFamily">sans-serif-condensed</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="Theme.Collapsing.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

</resources>